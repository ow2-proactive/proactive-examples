    <task name="Start_Serf_Cluster" 
    
    
    
    
    fork="true">
      <description>
        <![CDATA[ This task creates the first Serf agent that will allow later nodes to connect to it.  ]]>
      </description>
      <genericInformation>
        <info name="task.icon" value="/automation-dashboard/styles/patterns/img/wf-icons/controls_replicate.png"/>
        <info name="task.documentation" value="user/ProActiveUserGuide.html#_replicate"/>
        <info name="NODE_ACCESS_TOKEN" value="Edge#0"/>
      </genericInformation>
      <depends>
        <task ref="All_IP_Addresses_Collected"/>
      </depends>
      <scriptExecutable>
        <script>
          <code language="groovy">
            <![CDATA[
println "Creating a cluster of " +variables.get("numberEdges")+" Edge nodes"
]]>
          </code>
        </script>
      </scriptExecutable>
      <controlFlow >
        <replicate>
          <script>
            <code language="groovy">
              <![CDATA[
runs=variables.get("numberEdges")
]]>
            </code>
          </script>
        </replicate>
      </controlFlow>
      <post>
        <script>
          <code language="bash">
            <![CDATA[
sudo killall serf
sudo killall screen
sudo -u $USER bash -c "screen -dmS serf bash"
screen -S serf -p 0 -X stuff "serf agent -node=Edge#$variables_PA_TASK_REPLICATION -bind=0.0.0.0:7946 -advertise=$variables_IP0 ^M"

sleep 5
]]>
          </code>
        </script>
      </post>

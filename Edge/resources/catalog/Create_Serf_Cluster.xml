    <task name="Get_Node_Public_IP" 
    
    
    
    
    fork="true">
      <description>
        <![CDATA[ Gets the public IP of the node using a script in the utility scripts repo (https://raw.githubusercontent.com/ow2-proactive/utility-scripts/main/network-scripts/Get_public_ip.sh) ]]>
      </description>
      <genericInformation>
        <info name="task.icon" value="/automation-dashboard/styles/patterns/img/wf-icons/ip.png"/>
        <info name="NODE_ACCESS_TOKEN" value="Edge#${PA_TASK_REPLICATION}"/>
      </genericInformation>
      <depends>
        <task ref="Get_All_IP_Addresses"/>
      </depends>
      <scriptExecutable>
        <script>
          <code language="bash">
            <![CDATA[
SCRIPT_URL="https://raw.githubusercontent.com/ow2-proactive/utility-scripts/main/network-scripts/Get_public_ip.sh"
SCRIPT_DIR="/tmp/Get_public_ip.sh"

wget -q -O $SCRIPT_DIR $SCRIPT_URL

sudo chmod +x $SCRIPT_DIR

$SCRIPT_DIR  | tr -d '\n' > /tmp/ip.txt
]]>
          </code>
        </script>
      </scriptExecutable>
      <post>
        <script>
          <code language="groovy">
            <![CDATA[
String fileContents = new File('/tmp/ip.txt').text
variableName="IP"+variables.get("PA_TASK_REPLICATION")
variables.put(variableName,fileContents)

println variableName +" "+  variables.get(variableName)
]]>
          </code>
        </script>
      </post>

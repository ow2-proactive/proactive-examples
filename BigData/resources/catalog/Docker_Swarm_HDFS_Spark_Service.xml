      <scriptExecutable>
        <script>
          <code language="groovy">
            <![CDATA[
import org.ow2.proactive.scheduler.common.job.JobVariable

def signals = ['Submit_Job_To_Spark_Cluster','Undeploy_Platform']

// Manage the signal 'Undeploy_Platform'
signalapi.readyForSignal("Undeploy_Platform")

// Manage the signal 'Submit_Job_To_Spark_Cluster'
List <JobVariable> submitJobVariables = new java.util.ArrayList<JobVariable>()
submitJobVariables.add(new JobVariable("SPARK_WORKFLOW_CATALOG_PATH", "data-big-data/Python_Spark_Pi", "PA:CATALOG_OBJECT", "Catalog URL of the Spark workflow", "Spark Application Submission", false, false))
submitJobVariables.add(new JobVariable("SPARK_APPLICATION_PARAMETERS_JSON", "{\"nb_random_points\":10000}", "", "Arguments of the Spark application", "Spark Application Submission", false, false))
signalapi.readyForSignal("Submit_Job_To_Spark_Cluster", submitJobVariables)

// Receive the signal
receivedSignal = signalapi.waitForAny(signals.toSet())
println(receivedSignal)

signals.each {  signalapi.removeSignal("ready_"+it) }

if (receivedSignal.getName() == "Undeploy_Platform") {
    variables.put("SIGNAL_ACTION","UNDEPLOY_PLATFORM")
    println("Undeploying platform ...")

} else if (receivedSignal.getName() == "Submit_Job_To_Spark_Cluster"){
    variables.put("SIGNAL_ACTION","SUBMIT_JOB")
    variables.put("SPARK_WORKFLOW_CATALOG_PATH",receivedSignal.getUpdatedVariables().get("SPARK_WORKFLOW_CATALOG_PATH"))
    variables.put("SPARK_APPLICATION_PARAMETERS_JSON",receivedSignal.getUpdatedVariables().get("SPARK_APPLICATION_PARAMETERS_JSON"))
    println("Submitting Spark application to the cluster ...")
}
]]>
          </code>
        </script>

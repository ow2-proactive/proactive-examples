    <info name="workflow.icon" value="/automation-dashboard/styles/patterns/img/wf-icons/quantlib.png"/>
    <info name="Documentation" value="http://activeeon.com/resources/montecarlo-value-risk-proactive-activeeon.pdf"/>
  </genericInformation>

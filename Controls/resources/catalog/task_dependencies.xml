     name="Task_Dependencies">
    <description>
        <![CDATA[ A task that has two tasks dependencies, therefore it will be executed after the two first tasks finish. The two first tasks can run in parallel if enough resources are available. ]]>
    </description>
    <genericInformation>
        <info name="workflow.icon"
              value="/automation-dashboard/styles/patterns/img/wf-icons/controls_task_dependencies.png"/>
    </genericInformation>
    <taskFlow>
        <task name="a_task">
            <scriptExecutable>
                <script>
                    <code language="javascript">
                        <![CDATA[
result = "Hello";
]]>
                    </code>
                </script>
            </scriptExecutable>
        </task>
        <task name="another_task">
            <scriptExecutable>
                <script>
                    <code language="javascript">
                        <![CDATA[
result = "World";
]]>
                    </code>
                </script>
            </scriptExecutable>
        </task>
        <task name="dependent_task">
            <depends>
                <task ref="a_task"/>
                <task ref="another_task"/>
            </depends>
            <scriptExecutable>
                <script>
                    <code language="javascript">
                        <![CDATA[
print(results[0])
print(results[1])
]]>
                    </code>
                </script>
            </scriptExecutable>
        </task>
    </taskFlow>

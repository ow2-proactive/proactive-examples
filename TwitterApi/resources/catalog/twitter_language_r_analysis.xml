        <envScript>
          <script>
            <file language="groovy" url="${PA_CATALOG_REST_URL}/buckets/scripts/resources/fork_env_docker_args/raw">
              <arguments>
                <argument value="activeeon/demor"/>
              </arguments>
            </file>
          </script>
        </envScript>
